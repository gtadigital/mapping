<?xml version="1.0" encoding="UTF-8"?>
<generator_policy>
	<generator name="URIorUUID">
		<custom generatorClass="gr.forth.URIorUUID">
			<set-arg name="text"/>
		</custom>
	</generator>
	<generator name="coordinates">
		<pattern>POINT({lat} {long})</pattern>
	</generator>
	<generator name="coordinates_alternative">
		<pattern>{coordinates}</pattern>
	</generator>
	<generator name="URIwithType" prefix="sari">
		<pattern>{type}</pattern>
	</generator>
	<generator name="URIwithTypeAndId" prefix="sari">
		<pattern>{type}/{id}</pattern>
	</generator>
	<generator name="URIwithTypeAndId_hashed" prefix="sari">
		<pattern>{type}/{id}</pattern>
	</generator>
	<generator name="URIwith2TypesAndId" prefix="sari">
		<pattern>{type1}/{type2}/{id}</pattern>
	</generator>
	<generator name="timeSpan" prefix="sari">
		<pattern>event/timespan/{hash_time_span_value}</pattern>
	</generator>
	<!-- PLACE DEFINITION -->
	<generator name="placeAppellationUri" prefix="sari" shorten="true">
		<pattern>place/{place_id}/appellation/{appellation_id}</pattern>
	</generator>
	<generator name="placeIdentifierUri" prefix="sari">
		<pattern>place/{place_id}/{identifier_type}/{identifier}</pattern>
	</generator>
	<generator name="placeIdentifierUri-hashed" prefix="sari">
		<pattern>place/{place_id}/{identifier_type}/{identifier}</pattern>
	</generator>
	<generator name="actorIdentifier" prefix="sari">
		<pattern>place/{place_id}/identifier/{identifier_value}</pattern>
	</generator>
	<generator name="identifierAssignment" prefix="sari">
		<pattern>place/{place_id}/identifier_assignment/{identifier_value}</pattern>
	</generator>
	<!-- PERSON DEFINITION -->
	<generator name="personAppellationUri" prefix="sari" shorten="true">
		<pattern>person/{person_id}/appellation/{appellation_id}</pattern>
	</generator>
	<generator name="personIdentifierUri" prefix="sari">
		<pattern>person/{person_id}/{identifier_type}/{identifier}</pattern>
	</generator>
	<generator name="personIdentifierUri-hashed" prefix="sari">
		<pattern>person/{person_id}/{identifier_type}/{identifier}</pattern>
	</generator>
	<generator name="personBirth" prefix="sari">
		<pattern>person/{person_id}/birth</pattern>
	</generator>
	<generator name="personDeath" prefix="sari">
		<pattern>person/{person_id}/death</pattern>
	</generator>
	<generator name="personBiographyInfo" prefix="sari" uuid="true">
		<pattern>person/{person_id}/biography_information/{lang}/</pattern>
	</generator>
	<generator name="personBiography" prefix="sari" uuid="true">
		<pattern>person/{person_id}/biography/{lang}/</pattern>
	</generator>
	<!-- BUILT WORK / OEUVRE DEFINITION -->
	<generator name="builtworkAppellationUri" prefix="sari" shorten="true">
		<pattern>builtwork/{built_work_id}/appellation/{appellation_id}</pattern>
	</generator>
	<generator name="builtworkAlternativeAppellationUri" prefix="sari" shorten="true">
		<pattern>builtwork/{built_work_id}/alternative_appellation/{appellation_id}</pattern>
	</generator>
	<generator name="builtworkIdentifierUri" prefix="sari">
		<pattern>builtwork/{builtwork_id}/{identifier_type}/{identifier}</pattern>
	</generator>
	<generator name="builtworkIdentifierUri-hashed" prefix="sari">
		<pattern>builtwork/{builtwork_id}/{identifier_type}/{identifier}</pattern>
	</generator>
	<generator name="builtworkTimespanConstruction" prefix="sari">
		<pattern>builtwork/{builtwork_id}/construction/timespan</pattern>
	</generator>
	<generator name="builtworkTimespanDestruction" prefix="sari">
		<pattern>builtwork/{builtwork_id}/destruction/timespan</pattern>
	</generator>
	<generator name="builtworkCitationUUID" prefix="sari">
		<pattern>builtwork/{builtwork_id}/bibliographicitem/{identifier}</pattern>
	</generator>
	<generator name="builtworkCitation" prefix="sari">
		<pattern>builtwork/{builtwork_id}/bibliographicitem</pattern>
	</generator>
	<generator name="builtworkCreationPlace_id" prefix="sari">
		<pattern>builtwork/{builtwork_id}/place/{place_id}</pattern>
	</generator>
	<generator name="builtworkCreationPlace" prefix="sari">
		<pattern>builtwork/{builtwork_id}/place</pattern>
	</generator>
		<generator name="builtworkProduction" prefix="sari">
		<pattern>builtwork/{builtwork_id}/production</pattern>
	</generator>
	<generator name="builtworkDestruction" prefix="sari">
		<pattern>builtwork/{builtwork_id}/destruction</pattern>
	</generator>
		<!-- ARCHIVAL OBJECT -->
	<generator name="archivalobjectAppellationUri" prefix="sari" shorten="true">
		<pattern>archivalobject/{archivalobject_id}/appellation/{appellation_id}</pattern>
	</generator>
	<generator name="archivalobjectIdentifierUri" prefix="sari">
		<pattern>archivalobject/{archivalobject_id}/{identifier_type}/{identifier}</pattern>
	</generator>
	<generator name="archivalobjectIdentifierUri-hashed" prefix="sari">
		<pattern>archivalobject/{archivalobject_id}/{identifier_type}/{identifier}</pattern>
	</generator>
	<generator name="archivalobjectTimespanConstruction" prefix="sari">
		<pattern>archivalobject/{archivalobject_id}/construction/timespan</pattern>
	</generator>
	<generator name="archivalobjectTimespanDestruction" prefix="sari">
		<pattern>archivalobject/{archivalobject_id}/destruction/timespan</pattern>
	</generator>
	<generator name="archivalobjectCitationUUID" prefix="sari">
		<pattern>archivalobject/{archivalobject_id}/bibliographicitem/{identifier}</pattern>
	</generator>
	<generator name="archivalobjectCitation" prefix="sari">
		<pattern>archivalobject/{archivalobject_id}/bibliographicitem</pattern>
	</generator>
	<generator name="archivalobjectCreationPlace_id" prefix="sari">
		<pattern>archivalobject/{archivalobject_id}/place/{place_id}</pattern>
	</generator>
	<generator name="archivalobjectCreationPlace" prefix="sari">
		<pattern>archivalobject/{archivalobject_id}/place</pattern>
	</generator>
	<generator name="archivalobjectProduction" prefix="sari">
		<pattern>archivalobject/{archivalobject_id}/production</pattern>
	</generator>
	<generator name="archivalobjectDestruction" prefix="sari">
		<pattern>archivalobject/{archivalobject_id}/destruction</pattern>
	</generator>
	<!-- GROUP DEFINITION -->
	<generator name="groupIdentifierUri" prefix="sari">
		<pattern>group/{group_id}/{identifier_type}/{identifier}</pattern>
	</generator>
	<generator name="groupAppellationUri" prefix="sari" shorten="true">
		<pattern>group/{group_id}/appellation/{appellation_id}</pattern>
	</generator>
	<generator name="groupDissolution" prefix="sari">
		<pattern>group/{group_id}/dissolution</pattern>
	</generator>
	<generator name="groupDissolutionTimespan" prefix="sari">
		<pattern>group/{group_id}/dissolution/timespan</pattern>
	</generator>
	<generator name="groupFormation" prefix="sari">
		<pattern>group/{group_id}/formation</pattern>
	</generator>
	<generator name="groupFloruit" prefix="sari">
		<pattern>group/{group_id}/floruit</pattern>
	</generator>
	<generator name="groupFormationTimespan" prefix="sari">
		<pattern>group/{group_id}/formation/timespan</pattern>
	</generator>
	<generator name="groupBiography" prefix="sari" uuid="true">
		<pattern>group/{group_id}/biography/{lang}/</pattern>
	</generator>
	<generator name="URIwithGndId" prefix="gnd">
		<pattern>{id}</pattern>
	</generator>
	<generator name="URIwithUlanId" prefix="ulan">
		<pattern>{id}</pattern>
	</generator>
	<generator name="URIwithWikidataId" prefix="wikidata">
		<pattern>{id}</pattern>
	</generator>
	<generator name="URIwithTGNId" prefix="tgn">
		<pattern>{id}</pattern>
	</generator>
	<generator name="URIwithGeonamesId" prefix="geonames">
		<pattern>{id}</pattern>
	</generator>
	<generator name="URIwithGNDId" prefix="gnd">
		<pattern>{id}</pattern>
	</generator>
	<generator name="URIwithDbPediaId" prefix="dbpedia">
		<pattern>{id}</pattern>
	</generator>
	<generator name="URIwithVIAFId" prefix="viaf">
		<pattern>{id}</pattern>
	</generator>
	<generator name="URIwithSikartId" prefix="sikart">
		<pattern>{id}</pattern>
	</generator>
	<generator name="URIwithOneType" prefix="sari" uuid="true">
		<pattern>{type}/</pattern>
	</generator>
	<generator name="URIwithTransurbicide" prefix="tu">
		<pattern>{id}</pattern>
	</generator>
	
	<generator name="dateNormalizerGen">
		<custom generatorClass="gr.forth.DateNormalizer">
			<set-arg name="text"/>
			<set-arg name="bound"/>
			<set-arg name="report"/>
		</custom>
	</generator>
	<generator name="typedLiteralGen">
		<custom generatorClass="gr.forth.TypedLiteralGenerator">
			<set-arg name="text"/>
		</custom>
	</generator>
</generator_policy>